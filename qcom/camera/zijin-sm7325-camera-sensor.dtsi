        config {
            pins = "gpio20";
            bias-disable; /* No PULL */
            drive-strength = <2>; /* 2 MA */
        };
    };

    cam_sensor_suspend_front: cam_sensor_suspend_front {
        mux {
            pins = "gpio20";
            function = "gpio";
        };

        config {
            pins = "gpio20";
            bias-pull-down; /* PULL DOWN */
            drive-strength = <2>; /* 2 MA */
            output-low;
        };
    };

    cam_ultra_mclk_active: cam_ultra_mclk_active {
        mux {
            pins = "gpio67";
            function = "cam_mclk";
        };

        config {
            pins = "gpio67";
            bias-disable; /* No PULL */
            drive-strength = <4>; /* 4 MA */
        };
    };

    cam_ultra_mclk_suspend: cam_ultra_mclk_suspend {
        mux {
            pins = "gpio67";
            function = "cam_mclk";
        };

        config {
            pins = "gpio67";
            bias-pull-down; /* PULL DOWN */
            drive-strength = <4>; /* 4 MA */
        };
    };

    cam_sensor_active_ultra: cam_sensor_active_ultra {
        /*RESET */
        mux {
            pins = "gpio78";
            function = "gpio";
        };

        config {
            pins = "gpio78";
            bias-disable; /* No PULL */
            drive-strength = <2>; /* 2 MA */
        };
    };

    cam_sensor_suspend_ultra: cam_sensor_suspend_ultra {
        mux {
            pins = "gpio78";
            function = "gpio";
        };

        config {
            pins = "gpio78";
            bias-pull-down; /* PULL DOWN */
            drive-strength = <2>; /* 2 MA */
            output-low;
        };
    };

    cam_macro_mclk_active: cam_macro_mclk_active {
        mux {
            pins = "gpio66";
            function = "cam_mclk";
        };

        config {
            pins = "gpio66";
            bias-disable; /* No PULL */
            drive-strength = <4>; /* 2 MA */
        };
    };

    cam_macro_mclk_suspend: cam_macro_mclk_suspend {
        mux {
            pins = "gpio66";
            function = "cam_mclk";
        };

        config {
            pins = "gpio66";
            bias-pull-down; /* PULL DOWN */
            drive-strength = <4>; /* 2 MA */
        };
    };

    cam_sensor_active_macro: cam_sensor_active_macro {
        /*RESET */
        mux {
            pins = "gpio77";
            function = "gpio";
        };

        config {
            pins = "gpio77";
            bias-disable; /* No PULL */
            drive-strength = <2>; /* 2 MA */
        };
    };

    cam_sensor_suspend_macro: cam_sensor_suspend_macro {
        mux {
            pins = "gpio77";
            function = "gpio";
        };

        config {
            pins = "gpio77";
            bias-pull-down; /* PULL DOWN */
            drive-strength = <2>; /* 2 MA */
            output-low;
        };
    };
};
