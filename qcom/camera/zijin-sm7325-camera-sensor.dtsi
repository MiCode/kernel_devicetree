        cam_vdig-supply = <&L5I>;
        cam_vana-supply = <&L7I>;
        cam_clk-supply = <&cam_cc_titan_top_gdsc>;
        cam_bob-supply = <&BOB>;
        regulator-names = "cam_vio", "cam_vdig", "cam_vana", "cam_clk", "cam_bob";
        rgltr-cntrl-support;
