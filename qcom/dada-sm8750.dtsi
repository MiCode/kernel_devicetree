&usb0 {
	qcom,wcd_usbss = <&wcd_usbss>;
	wcd_equ = <0xC0>;
	wcd_equ_host = <0xC0>;
	maximum_speed = "super-speed";
	host-force-gen1;
};
&pmih010x_eusb2_repeater {
        qcom,host-param-override-seq =
                <0x00 0x50
                0x0F 0x51
                0x05 0x52
                0x07 0x53
                0x03 0x54
                0x03 0x55
                0x00 0x56
                0x03 0x57
                0x02 0x58
                0x03 0x59
                0x01 0x5a
                0x02 0x5b>;
};
&usb_qmp_dp_phy {
	usb3,eyegram-tuning;
	qcom,usb3-phy-eye-seq =
	/* <reg_offset, value>*/
		<USB3_DP_PCS_G12S1_TXMGN_V0 0x1F
			USB3_DP_PCS_G12S1_TXDEEMPH_M3P5DB 0x18>;
	qcom,host-usb3-phy-eye-seq =
		<USB3_DP_PCS_G12S1_TXMGN_V0 0x1F
			USB3_DP_PCS_G12S1_TXDEEMPH_M3P5DB 0x17>;
};

