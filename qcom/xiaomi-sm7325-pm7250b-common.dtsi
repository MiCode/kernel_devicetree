		interrupts = <45 0>;
		interrupt-names = "smartpa_irq";
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&smartpa_bot_int_active>;
		pinctrl-1 = <&smartpa_bot_int_suspend>;
		reg = <0x35>;
		status = "ok";
	};
};

