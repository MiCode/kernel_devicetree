		qcom,adc-tm-type = <1>;
	};

	pm8550vs_d_die_temp {
		reg = <PM8550VX_ADC5_GEN3_DIE_TEMP(3)>;
		label = "pm8550vs_d_die_temp";
		qcom,pre-scaling = <1 1>;
	};

	pm8550vs_g_die_temp {
		reg = <PM8550VX_ADC5_GEN3_DIE_TEMP(6)>;
