&thermal_zones {
	xo-therm-adc {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6155_adc_tm ADC5_XO_THERM_100K_PU>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};

			reset-mon-1-cfg {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	pm6155-1-tz {
		cooling-maps {
			trip0_cpu0 {
				trip = <&pm6155_trip0>;
				cooling-device =
					<&CPU0 THERMAL_MAX_LIMIT
						THERMAL_MAX_LIMIT>;
			};

			trip0_cpu1 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu1_pause 1 1>;
			};

			trip0_cpu2 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu2_pause 1 1>;
			};

			trip0_cpu3 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu3_pause 1 1>;
			};

			trip0_cpu4 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu4_pause 1 1>;
			};

			trip0_cpu5 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu5_pause 1 1>;
			};

			trip0_cpu6 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu6_pause 1 1>;
			};

			trip0_cpu7 {
				trip = <&pm6155_trip0>;
				cooling-device = <&cpu7_pause 1 1>;
			};
		};
	};
};

&pm6155_1_rtc {
	disable-alarm-wakeup;
};
