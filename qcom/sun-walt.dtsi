&soc {
	walt {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		qcom,cycle-cntr {
			compatible = "qcom,gclk";
			reg = <0x18880400 0x8>,
				<0x19880400 0x8>;
			reg-names = "freq-domain0",
				"freq-domain1";
		};
	};
};
