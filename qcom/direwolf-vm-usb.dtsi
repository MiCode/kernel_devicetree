			USB3_QSERDES_PLL_SSC_STEP_SIZE1_MODE1	0xFD
			USB3_QSERDES_PLL_SSC_STEP_SIZE2_MODE1	0x0D
			USB3_QSERDES_PLL_SYSCLK_BUF_ENABLE	0x0A
			USB3_QSERDES_PLL_CP_CTRL_MODE0	0x02
