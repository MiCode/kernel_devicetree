			USB3_QSERDES_PLL_PLL_RCTRL_MODE1	0x16
			USB3_QSERDES_PLL_PLL_CCTRL_MODE0	0x36
			USB3_QSERDES_PLL_PLL_CCTRL_MODE1	0x36
			USB3_QSERDES_PLL_SYSCLK_EN_SEL	0x1A
			USB3_QSERDES_PLL_LOCK_CMP_EN	0x04
