			 USB43DP_COM_TYPEC_CTRL
			 USB3_PCS_CLAMP_ENABLE
			 USB43DP_COM_TYPEC_STATUS>;

		qcom,qmp-phy-init-seq =
			/* <reg_offset, value> */
