&pm8550vs_c_gpios {
	key_back {
		key_back_default: key_back_default {
			pins = "gpio2";
			function = "normal";
			input-enable;
			bias-pull-up;
			power-source = <1>;
		};
	};

	key_confirm {
		key_confirm_default: key_confirm_default {
			pins = "gpio5";
			function = "normal";
			input-enable;
			bias-pull-up;
			power-source = <1>;
		};
	};

	key_vol_up {
		key_vol_up_default: key_vol_up_default {
			pins = "gpio6";
			function = "normal";
			input-enable;
			bias-pull-up;
			power-source = <1>;
		};
	};
};

&spmi_bus {
	#address-cells = <2>;
	#size-cells = <0>;
	interrupt-controller;
	#interrupt-cells = <4>;

	qcom,pm8550b_i@8 {
		compatible = "qcom,spmi-pmic";
		reg = <8 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;

		pm8550b_i_tz: pm8550b-i-temp-alarm@a00 {
			compatible = "qcom,spmi-temp-alarm";
			reg = <0xa00>;
			interrupts = <0x8 0xa 0x0 IRQ_TYPE_EDGE_BOTH>;
			#thermal-sensor-cells = <0>;
		};

		pm8550b_i_lite_tz: pm8550b-i-temp-alarm-lite@c00 {
			compatible = "qcom,spmi-temp-alarm";
			reg = <0xc00>;
			interrupts = <0x8 0xc 0x0 IRQ_TYPE_EDGE_BOTH>;
			#thermal-sensor-cells = <0>;
		};

		pm8550b_i_gpios: pinctrl@8800 {
			compatible = "qcom,pm8550b-gpio";
			reg = <0x8800>;
			gpio-controller;
			#gpio-cells = <2>;
			interrupt-controller;
			#interrupt-cells = <2>;
		};

		pm8550b_eusb2_sec_repeater: qcom,eusb2-sec-repeater@fd00 {
			status = "disabled";
			compatible = "qcom,pmic-eusb2-repeater";
			reg = <0xfd00>;
		};
	};

	pm8550ve_d:
	qcom,pm8550ve_d@3 {
		compatible = "qcom,spmi-pmic";
		reg = <3 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;

		pm8550ve_d_tz: pm8550ve-d-temp-alarm@a00 {
			compatible = "qcom,spmi-temp-alarm";
			reg = <0xa00>;
			interrupts = <0x3 0xa 0x0 IRQ_TYPE_EDGE_BOTH>;
			#thermal-sensor-cells = <0>;
			io-channels = <&pmk8550_vadc PM8550VE_D_ADC5_GEN3_DIE_TEMP>;
			io-channel-names = "thermal";
		};

		pm8550ve_d_gpios: pinctrl@8800 {
			compatible = "qcom,pm8550ve-gpio";
			reg = <0x8800>;
			gpio-controller;
			#gpio-cells = <2>;
			interrupt-controller;
			#interrupt-cells = <2>;
		};
	};

	qcom,pmk8550@0 {
		qti_epm: qcom,epm-device {
			compatible = "qcom,epm-devices";
			nvmem = <&pmk8550_sdam_49>,
				<&pmk8550_sdam_53>;
			nvmem-names = "epm-config-sdam",
				"epm-10s-avg-sdam";

			qcom,reg-ppid-ids = /bits/ 16 <0x19e 0x1a1 0x1aa
					0x29e 0x2a1 0x2a4 0x2aa 0x3a1 0x3a4 0x3a7 0x3aa
					0x3ad 0x59b 0x5a1 0x5a4 0x5aa 0x5ad 0x5a7>;

			qcom,reg-ppid-names = "mv_sub_reg",
					"mm", "dpu", "ebi", "vddq", "nsp2", "nsp1",
					"gfx_mx", "mxc", "wlan_cx", "lv_sub_reg",
					"gfx", "apc0", "mxa", "vdd2h", "cx", "apc1",
					"vdd2l";
		};
	};
};

