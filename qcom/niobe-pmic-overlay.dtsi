		nvmem-cell-names = "ocp_log";
		status = "disabled";
	};
};

&thermal_zones {
	pm8550b_i_temp_alarm: pm8550b_i_tz {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8550b_i_tz>;

		trips {
			pm8550b_i_trip0: trip0 {
				temperature = <95000>;
				hysteresis = <0>;
				type = "passive";
			};

			pm8550b_i_trip1: trip1 {
				temperature = <115000>;
				hysteresis = <0>;
				type = "passive";
			};

			pm8550b_i_trip2: trip2 {
				temperature = <145000>;
				hysteresis = <0>;
				type = "critical";
			};
		};
	};

	pm8550b_i_lite_temp_alarm: pm8550b_i_lite_tz {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8550b_i_lite_tz>;

		trips {
			trip0 {
				temperature = <125000>;
				hysteresis = <0>;
				type = "passive";
			};

			trip1 {
				temperature = <135000>;
				hysteresis = <0>;
				type = "passive";
			};

			trip2 {
				temperature = <145000>;
				hysteresis = <0>;
				type = "critical";
			};
		};
	};

	pm8550ve_d_temp_alarm: pm8550ve_d_tz {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8550ve_d_tz>;

