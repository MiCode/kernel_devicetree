/* Config uart for ASIC debug start */
&aliases {
	hsuart1 = &qupv3_se6_2uart;
};


&tlmm {
	qupv3_se6_2uart_pins: qupv3_se6_2uart_pins {
		qupv3_se6_2uart_tx_active: qupv3_se6_2uart_tx_active {
			mux {
				pins = "gpio58";
				function = "qup1_se6_l2";
			};

			config {
				pins = "gpio58";
				drive-strength = <2>;
				bias-disable;
			};
		};

		qupv3_se6_2uart_rx_active: qupv3_se6_2uart_rx_active {
			mux {
				pins = "gpio59";
				function = "qup1_se6_l3";
			};

			config {
				pins = "gpio59";
				drive-strength = <2>;
				bias-disable;
			};
		};

		qupv3_se6_2uart_sleep: qupv3_se6_2uart_sleep {
			mux {
				pins = "gpio58", "gpio59";
				function = "gpio";
			};

			config {
				pins = "gpio58", "gpio59";
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};
};

&qupv3_1 {
	qupv3_se6_2uart: qcom,qup_uart@a98000 {
		compatible = "qcom,msm-geni-serial-hs";
		reg = <0xa98000 0x4000>;
		reg-names = "se_phys";
		interrupts-extended = <&intc GIC_SPI 363 IRQ_TYPE_LEVEL_HIGH>,
				<&tlmm 59 IRQ_TYPE_LEVEL_HIGH>;
		clock-names = "se-clk";
		clocks = <&gcc GCC_QUPV3_WRAP1_S6_CLK>;
		interconnect-names = "qup-core", "qup-config", "qup-memory";
		interconnects =
		<&clk_virt MASTER_QUP_CORE_1 &clk_virt SLAVE_QUP_CORE_1>,
		<&gem_noc MASTER_APPSS_PROC &config_noc SLAVE_QUP_1>,
		<&aggre1_noc MASTER_QUP_1 &mc_virt  SLAVE_EBI1>;
		pinctrl-names = "default", "active", "sleep", "shutdown";
		pinctrl-0 = <&qupv3_se6_2uart_tx_active>, <&qupv3_se6_2uart_rx_active>;
		pinctrl-1 = <&qupv3_se6_2uart_tx_active>, <&qupv3_se6_2uart_rx_active>;
		pinctrl-2 = <&qupv3_se6_2uart_sleep>;
		pinctrl-3 = <&qupv3_se6_2uart_sleep>;
		qcom,auto-suspend-disable;
		status = "ok";
	};
};
/* Config uart for ASIC debug end */
