		<&gem_noc MASTER_APPSS_PROC &system_noc SLAVE_SMBUS_CFG>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&i2c_slave_sda_active>, <&i2c_slave_scl_active>;
		pinctrl-1 = <&i2c_slave_sleep>;
		status = "disabled";
	};
