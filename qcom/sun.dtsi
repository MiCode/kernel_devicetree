				qcom,cpufreq-memfreq-tbl =
					<  880000  350000 >,
					< 1670000  533000 >,
					< 2270000  806000 >,
					< 2580000  933000 >;
				qcom,sampling-enabled;
			};

			prime {
				compatible = "qcom,memlat-mon";
				qcom,cpulist = <&CPU6 &CPU7>;
				qcom,cpufreq-memfreq-tbl =
