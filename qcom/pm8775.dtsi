			};

			pm8775_2_vph_pwr {
				reg = <PM8775_2_ADC5_GEN3_VPH_PWR>;
				label = "pm8775_2_vph_pwr";
				qcom,pre-scaling = <1 3>;
			};
		};

		pon_pbs_2: pon_pbs@800 {
			compatible = "qcom,qpnp-power-on";
