			interrupts = <0x2 0x80 0x1 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "adc-sdam0";
			#io-channel-cells = <1>;
			status = "disabled";

			pm8775_2_ref_gnd {
				reg = <PM8775_2_ADC5_GEN3_OFFSET_REF>;
				label = "pm8775_2_ref_gnd";
				qcom,pre-scaling = <1 1>;
