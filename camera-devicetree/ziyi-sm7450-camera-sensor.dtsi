		rgltr-load-current = <1067000 7840000>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_soft_led_en_active>;
		pinctrl-1 = <&cam_soft_led_en_suspend>;
		gpios = <&tlmm 15 0>;
