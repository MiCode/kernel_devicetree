	cam_sensor_suspend_rst3: cam_sensor_suspend_rst3 {
		/* RESET MACRO */
		mux {
			pins = "gpio78";
			function = "gpio";
		};

		config {
			pins = "gpio78";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};


	//front ultra reset
	cam_sensor_active_rst4: cam_sensor_active_rst4 {
		/* RESET FRONT ULTRA*/
		mux {
			pins = "gpio80";
			function = "gpio";
		};

		config {
			pins = "gpio80";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
